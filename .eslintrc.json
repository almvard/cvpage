{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-multiple-empty-lines": ["warn"],
        "react/jsx-filename-extension": "off",
        "react/require-default-props": "off",
        "import/prefer-default-export": "off"
    }
}
